{
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "scripts": {
    "dev": "tsx watch downloader.ts",
    "start": "tsx downloader.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc && node dist/downloader.js",
    "dev:strict": "concurrently \"npm run typecheck\" \"npm run dev\"",
    "docs": "typedoc"
  },
  "nodemonConfig": {
    "ignore": [
      "*.m4a",
      "*.mp4",
      "*.flac",
      "*.mp3",
      "test.html",
      "playinfo.json"
    ],
    "ext": "ts,js,json"
  },
  "bin": "./downloader.js",
  "pkg": {
    "assets": [
      "node_modules/chalk/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64"
    ]
  },
  "name": "bilibili-audio-downloader",
  "version": "0.0.1",
  "description": "Download audio from Bilibili videos",
  "author": "kun",
  "license": "MIT"
}

{
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0"
  },
  "scripts": {
    "dev": "tsx watch downloader.ts",
    "start": "tsx downloader.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc && node dist/downloader.js",
    "dev:strict": "concurrently \"npm run typecheck\" \"npm run dev\""
  },
  "nodemonConfig": {
    "ignore": [
      "*.m4a",
      "*.mp4",
      "*.mp3",
      "test.html",
      "playinfo.json"
    ],
    "ext": "ts,js,json"
  }
}
